{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Enables the default permissions and HTTP access",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost:5270/api/sqlfile" }
      ]
    }
  ]
}
